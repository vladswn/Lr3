<div class="container-fluid">
    <div class="row">
        <div class="col-md-12" style="padding:0px;margin-top:100px;">
            <div class="img-courses">
                <img src="http://funkyview.com/wp-content/uploads/2015/10/Technology.jpg" class="img-responsive" style="height: 260px;width:100%;" />
            </div>
        </div>
    </div>
</div>
<div class="container" style="margin-top:80px;">
    <div class="row">
        <hr />
        @*<div class="col-md-8">*@
        <div class="col-lg-8"> 
            <div class="btn-group btn-group-lg" role="group" aria-label="...">
                @Html.ActionLink("Усі курси", "Courses", "Home", new { sortCourse = ViewBag.AllCourse }, new { @class = "btn btn-primary" })
                @Html.ActionLink("Поточні курси", "Courses", "Home", new { sortCourse = ViewBag.CourseNow }, new { @class= "btn btn-primary" })
                @Html.ActionLink("Заплановані курси", "Courses", "Home", new { sortCourse = ViewBag.FutureCourse }, new { @class = "btn btn-primary" })
                @Html.ActionLink("Минулі курси", "Courses", "Home", new { sortCourse = ViewBag.PastCourse }, new { @class = "btn btn-primary" })
            </div>
        </div>
        <div class="col-lg-4">

            @using (Html.BeginForm("Courses","Home",FormMethod.Get))
            {
                <div class="input-group" style="margin-top:6px;">
                    <span class="input-group-btn">
                        <button class="btn btn-default" type="submit">
                            <i class="glyphicon glyphicon-search"></i>
                        </button>
                    </span>
                    <input type="text" name="search" class="form-control" placeholder="Знайти курс...">
                </div>
            }
            
            
        </div>
        
    </div>
    <hr />


    <div class="row">
        @*<div class="col-sm-2" style="border:1px solid black;">
                some text
                <br />
                some text
                <br />
