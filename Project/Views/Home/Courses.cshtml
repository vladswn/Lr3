        <div class="col-sm-12" style="background: #e6e6e6;padding-bottom:15px;">
            <h2 class="text-danger text-center">@ViewBag.MessageResult</h2>
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="row">
                    @foreach (var item in Model)
                    {
                        if (item.SrartDate > DateTime.Now)
                        {
                            <div class="col-sm-3 my_cl">
                                <div class="thumbnail  cours_lst" style="border-bottom:5px solid #4bb4fb;">
                                    @*<img src="data:image/svg+xml;base64," class="img-responsive" />*@
                                    @if (item.ImagePath != null)
                                    {
                                        <img src="@Url.Content(item.ImagePath)" alt="image" class="img-responsive" style="height:180px;padding-left:5px;padding-right:5px;" />
                                    }
                                    else
                                    {
                                        <img src="data:image/svg+xml;base64," class="img-responsive" style="height:180px;" />
                                    }
                                    <div class="caption" style="padding-top:0px;">
                                        <pre class="pre-header" style="overflow: hidden;">
                                             <h3>@item.Title</h3>
                                        </pre>
                                        <p>
                                        <p>Заплановані курси</p>
                                        <p>Початок:@item.SrartDate.ToShortDateString()</p>
                                        </p>
                                        <p>
                                            @Html.ActionLink("Опис курсу", "DetailCourse", new { id = item.CourseId }, new { @class = "btn btn-primary btn-block" })
                                        </p>
                                    </div>
                                </div>
                            </div>


                        }
                        else if (item.SrartDate < DateTime.Now && item.EndDate < DateTime.Now)
                        {
                            <div class="col-sm-3 my_cl">
                                <div class="thumbnail cours_lst" style="border-bottom:5px solid #008000;padding:0px;padding-top:5px;">
                                    @*<img src="data:image/svg+xml;base64," class="img-responsive" />*@
                                    @if (item.ImagePath != null)
                                    {
                                        <img src="@Url.Content(item.ImagePath)" alt="image" class="img-responsive" style="height:180px;padding-left:5px;padding-right:5px;" />
                                    }
                                    else
                                    {
                                        <img src="data:image/svg+xml;base64," class="img-responsive" style="height:180px;" />
                                    }
                                    <div class="caption" style="padding-top:0px;">
                                        <pre class="pre-header" style="overflow: hidden;">
                                        <h3>@item.Title</h3>
                                    </pre>
                                        <p>
                                        <p>Минулі курси</p>
                                        <p>Початок:@item.SrartDate.ToShortDateString()</p>
                                       
                                        <p>
                                            @Html.ActionLink("Опис курсу", "DetailCourse", new { id = item.CourseId }, new { @class = "btn btn-primary btn-block" })
                                        </p>
                                    </div>
                                </div>
                            </div>

                        }
                        else
                        {
                            <div class="col-sm-3 my_cl" style="margin-top:20px;">
                                <div class="thumbnail  cours_lst" style="border-bottom:5px solid #FFA500;">
                                    @*<img src="data:image/svg+xml;base64," class="img-responsive" />*@
                                    @if (item.ImagePath != null)
                                    {
                                        <img src="@Url.Content(item.ImagePath)" alt="image" class="img-responsive" style="height:180px;padding-left:5px;padding-right:5px;" />
                                    }
                                    else
                                    {
                                        <img src="data:image/svg+xml;base64," class="img-responsive" style="height:180px;" />
                                    }
                                    <div class="caption" style="padding-top:0px;">
                                        <pre class="pre-header" style="overflow: hidden;">
                                        <h3>@item.Title</h3>
                                    </pre>
                                        <p>
                                                <p>Поточні курси</p>
                                                <p>Початок:@item.SrartDate.ToShortDateString()</p>
                                            </p>
                                            <p>
                                                @Html.ActionLink("Опис курсу", "DetailCourse", new { id = item.CourseId }, new { @class = "btn btn-primary btn-block" })
                                            </p>
                                        </div>
                                    </div>
                                </div>
                        }


                        @*<div class="col-sm-3" style="margin-top:20px;">
                            <div class="thumbnail  my_cl">
